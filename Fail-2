startInterval=int(input("Choose the starting number for the interval you want to find a root."))
endInterval=int(input("Choose the end number for the interval you want to find a root."))

def f(input):
    while(inp<endInterval):
        output=-1.278*input*input+5.926*input-2.771
        return output
    
L=[]
stepSize=0.001

inp=startInterval/2
inp2=startInterval/2

while(inp<=endInterval):
    if f(inp-stepSize)*f(inp)<0 and f(inp)*f(inp+stepSize)>0 :
        L.append(inp)
    inp=inp+stepSize
print(L)

while(inp2>=startInterval):
    if f(inp-stepSize)*f(inp)<0 and f(inp)*f(inp+stepSize)>0 :
        L.append(inp)
    inp=inp-stepSize
print(L)





    





    

