import math 
startInterval=input("Choose the starting number for the interval you want to find a root.")
endInterval=input("Choose the end number for the interval you want to find a root.")

def f(input):
    output=-1.278*input**2+5.926*input-2.771
    return output


L=[]
stepSize=0.001
num_steps=0



while(not float(endInterval)-float(startInterval)<0.001):
    if(math.fabs(f(startInterval)<0.001)):
       L.append(startInterval)
    startInterval+=stepSize
    num_steps+=1
     
print(L)






    





    
